                <!-- <th>特別註記</th> -->
              </tr>
              </thead>
              <tbody id="caselist">
                  <tr v-for="casex of caselist" :key="casex._id">
                  <td>{{casex.a15casename}}</td>
                  <td>{{casex.a20caseaddress}}</td>
                  <!-- statusreport="由已擷取資料組清單頁進入本頁"> -->

                    <td>
                      <input type="submit" value="檢視申請資料" @click="viewone(casex._id)">
                    </td>
                    <td>
                      <input type="submit" value="進度查詢" @click="progress(casex._id)">
                    </td>
                  </tr>
              </tbody>
          </table>
