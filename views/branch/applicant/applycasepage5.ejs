          item9:"申請案獲得之碳權噸數",
          item99:"特別註記"
            }
        }
      },
      mounted:function(){

      },
      methods:{
        viewone:function(id){
          let querytext="?statusreport=由申請中case資料清單頁的檢視鈕進入本頁&status=5&caseID="+id;
          let url3='/base4dcarbon/case/caseinfo/'+this.personID+querytext;
          window.location=url3;
        },
        progress:function(id){
          let querytext="?statusreport=由申請中case資料清單頁查詢進度鈕進入本頁&status=5&caseID="+id;
          let url3='/base4dcarbon/progress/applycase/'+this.personID+querytext;
          window.location=url3
           }
      },
    template:"#content1UI"
  });

