<script>
  Vue.component("content1",{
    props:{},
    data(){
        return{

          statusreport:'<%- statusreport %>',
          personID:'<%- personID %>',
          caselist:JSON.parse(decodeURIComponent('<%= caselist %>')),
        items:{
          item0:"申請人代碼",
          item1:"申請活動代碼",
          item2:"申請案場名稱",
          item3:"申請案場地址",
