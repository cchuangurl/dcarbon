<script src="https://cdn.bootcss.com/qs/6.5.1/qs.min.js"></script>
  <script>
    Vue.component("outputer",{
      props:{},
      data(){
        return{
          statusreport:'<%- JSON.stringify(statusreport) %>',
          personID:'<%- personID %>',
          caseID:'<%- caseID %>',
          termlist:JSON.parse(decodeURIComponent('<%=termlist%>')),
          progresslist:JSON.parse(decodeURIComponent('<%=progresslist%>')),
        items:{
          item0:"申請案代碼",
          item1:"進度名稱",
          item2:"進度時間",
          item99:"特別註記"
            }
        }
      },
      mounted:function(){
        for(progress of this.progresslist){
              let temp=this.termlist.find(obj=>obj.a25code==progress.a10stage)
              progress.a10stage=temp.a30mean;
              }
        this.progresslist.sort((a,b)=>{
            return a.a15when-b.a15when
        })
      },
      methods:{
        editone:function(id){
          let querytext="?statusreport=由progress查詢結果頁的檢視按鈕進入本頁";
          let url2='/base4dcarbon/progress/editpage/'+this.personID+querytext;
          window.location=url2;
        },
        cowork:function(id){
          let querytext="?statusreport=由progress查詢結果頁的需配合事項按鈕進入本頁&caseID="+this.caseID;
          let url3='/base4dcarbon/case/cowork/'+this.person+querytext;
          window.location=url3
           },

      },
    template:"#outputerUI"
  });
