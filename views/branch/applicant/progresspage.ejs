                <td>{{progress.a10stage}}</td>
                <td>{{progress.a15when}}</td>
                <!-- statusreport="由已擷取資料組清單頁進入本頁"> -->

                  <td>
                    <input type="submit" value="檢視" @click="editone(progress._id)">
                  </td>
                  <td>
                    <input type="submit" value="需配合事項" @click="cowork(progress._id)">
                  </td>
